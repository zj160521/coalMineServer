<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.cm.dao.AnalogMeasureDao">
	
	<update id="add" parameterType="com.cm.entity.Measure">
		UPDATE t_analoginfo_query SET measure=#{measure},measuretime=#{measuretime} where id=#{id}
	</update>
	
</mapper>